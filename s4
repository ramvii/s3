#include <stdio.h>
#include <stdlib.h>

int main()
{
    printf("          a        \n");
    printf("         a a       \n");
    printf("        a   a      \n");
    printf("       a     a     \n");
    printf("      aaaaaaaaa    \n");
    printf("     a         a   \n");
    printf("    a           a  \n");
    printf("   a             a \n");
    printf("  a               a  ");

return 0;
}
